#include <iostream>
using namespace std;

int main() {
    float precio, iva, total;

    cout << "Precio del producto: ";
    cin >> precio;

    cout << "Porcentaje de IVA (ej. 16): ";
    cin >> iva;

    total = precio + (precio * iva / 100);

    cout << "El precio final es: " << total << endl;

    return 0;
}
